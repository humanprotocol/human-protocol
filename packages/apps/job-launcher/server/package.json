{
  "name": "@human-protocol/job-launcher-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "migration:create": "yarn build && typeorm-ts-node-commonjs migration:create",
    "migration:generate": "yarn build && typeorm-ts-node-commonjs migration:generate -p -d typeorm.config.ts",
    "migration:revert": "yarn build && typeorm-ts-node-commonjs migration:revert -d typeorm.config.ts",
    "migration:run": "yarn build && typeorm-ts-node-commonjs migration:run -d typeorm.config.ts",
    "migration:show": "yarn build && typeorm-ts-node-commonjs migration:show -d typeorm.config.ts",
    "docker:db:up": "docker compose up -d postgres && yarn migration:run",
    "docker:db:down": "docker compose down",
    "setup:local": "ts-node ./test/setup.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "export POSTGRES_DATABASE=job-launcher POSTGRES_PASSWORD=qwerty POSTGRES_USER=default POSTGRES_HOST=0.0.0.0 NODE_ENV=test-e2e && docker compose up -d postgres && yarn migration:run && jest --config ./test/jest-e2e.json; testStatus=$?; docker rm -f postgres; exit $testStatus"
  },
  "dependencies": {
    "@human-protocol/sdk": "*",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.2.7",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.8",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.3.9",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/serve-static": "^4.0.1",
    "@nestjs/swagger": "^7.1.13",
    "@nestjs/terminus": "^10.2.1",
    "@nestjs/typeorm": "^10.0.1",
    "@sendgrid/mail": "^8.1.3",
    "@types/cookie-parser": "^1.4.3",
    "@types/express-session": "^1.17.10",
    "@types/passport-jwt": "^4.0.1",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "cookie-parser": "^1.4.6",
    "decimal.js": "^10.4.3",
    "express-session": "^1.17.3",
    "joi": "^17.12.2",
    "json-stable-stringify": "^1.1.1",
    "nestjs-minio-client": "^2.2.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "8.12.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "stripe": "^14.24.0",
    "typeorm": "^0.3.17",
    "typeorm-naming-strategies": "^4.1.0",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.5.0",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/testing": "^10.3.1",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.12",
    "@types/json-stable-stringify": "^1.0.36",
    "@types/node": "20.12.12",
    "@types/supertest": "^6.0.2",
    "@types/xml2js": "0.4.13",
    "@types/zxcvbn": "4.4.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vercel/node": "^3.0.26",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "29.7.0",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.20",
    "supertest": "^6.3.4",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.0.0"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
