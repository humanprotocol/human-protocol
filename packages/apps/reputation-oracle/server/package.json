    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "8.13.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.16",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^11.1.0",
    "zxcvbn": "^4.4.2"
