    "@nestjs/platform-express": "^10.3.10",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/typeorm": "^10.0.1",
    "@sendgrid/mail": "^8.1.3",
    "@slack/bolt": "^4.2.1",
    "@slack/web-api": "^7.9.1",
    "@slack/webhook": "^7.0.5",
    "@types/passport-jwt": "^4.0.1",
    "axios": "^1.8.1",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
