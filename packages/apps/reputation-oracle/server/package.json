    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@types/zxcvbn": "4.4.5",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "29.7.0",
    "nock": "^14.0.3",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.20",
    "supertest": "^7.0.0",
    "ts-jest": "29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
