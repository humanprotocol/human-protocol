{
  "version": 2,
  "builds": [
    {
      "src": "index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/jobs/solutions",
      "methods": ["POST"],
      "dest": "index.ts",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "POST,OPTIONS",
        "Access-Control-Allow-Headers": "X-Requested-With,Content-Type,Accept"
      }
    },
    {
      "src": "/(.*)",
      "dest": "index.ts",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "*",
        "Access-Control-Allow-Headers": "X-Requested-With,Content-Type,Accept"
      }
    }
  ],
  "ignoreCommand": "git diff HEAD^ HEAD --quiet -- ./packages/core/contracts ./packages/sdk/typescript ./packages/examples/fortune/exchange-oracle/server"
}
