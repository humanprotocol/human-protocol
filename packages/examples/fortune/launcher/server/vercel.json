{
  "buildCommand": "yarn workspace @human-protocol/fortune-job-launcher-server build",
  "installCommand": "yarn install",
  "outputDirectory": "build",
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ]
}
